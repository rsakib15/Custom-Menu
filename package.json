    "@idg/fastclick": "^0.1.1",
    "@idg/idg": "^2.12.36",
    "@idg/iview": "^4.7.31",
    "@idg/webpack": "^0.0.127",
    "@sentry/tracing": "^6.2.0",
    "@sentry/vue": "^6.2.5",
    "axios": "^0.19.0",
    "babel-core": "7.0.0-bridge.0",
    "chalk": "^3.0.0",
    "commitizen": "^4.1.2",
    "conventional-changelog": "^3.1.21",
    "cz-customizable": "^6.2.0",
    "husky": "^4.2.5",
    "insert-css": "^2.0.0",
    "localforage": "^1.9.0",
    "lodash": "^4.17.15",
